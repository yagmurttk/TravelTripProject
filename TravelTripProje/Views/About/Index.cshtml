@using TravelTripProje.Models.Siniflar
@model List<Hakkimizda>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkımda</h3>
            @foreach (var x in Model)
            {
                <img src="@x.FotoUrl" alt="" />

                <p>@x.Aciklama</p>
            }

        </div>
    </div>
</div>
